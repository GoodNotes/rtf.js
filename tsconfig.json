{
    "compilerOptions": {
        "outDir": "./dist/src",
        "allowJs": false,
        "target": "ES5",
        "lib": [
            "ES2018", "DOM"
        ],
        "baseUrl": "src",
        "paths": {
            "EMFJS": ["emfjs/index.ts"],
            "WMFJS": ["wmfjs/index.ts"]
        },
        "moduleResolution": "node",
        "module": "ES2015",
        "sourceMap": true,
        "declaration": true,
        "strict": true,
        "strictNullChecks": false
    }
}
